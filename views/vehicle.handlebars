<div class="container">
  <div class="row text-center d-flex align-items-stretch">
  {{#each vehicleData as |car|}}

    <div class="col-md-4 mb-4 mb-md-3 d-flex align-items-stretch">
      <div class="card testimonial-card">
        <div class="card-up" style="background-color: #9d789b;"></div>
        <div class="card-body">
          <h4>{{brand}} {{model}} <small class="text-muted">{{year}}</small></h4>
          <hr />
          <p class="dark-grey-text mt-4">
            Available at <a href="/location/{{location_id}}">{{ location.name }}</a>
          </p>
          <img src="./assets/img/{{model}}/{{model}}.png" style="width: 100%;">
        </div>
      </div>
    </div>

  {{/each}}
  </div>
</div>